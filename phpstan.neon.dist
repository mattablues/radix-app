includes:
    - vendor/phpstan/phpstan-phpunit/extension.neon

parameters:
    level: 10
    paths:
        - bootstrap
        - src
        - tests
        - config
        - public
        - support
    reportUnmatchedIgnoredErrors: true
    inferPrivatePropertyTypeFromConstructor: true
    treatPhpDocTypesAsCertain: false

    bootstrapFiles:
        - tools/phpstan-bootstrap.php
