parameters:
    level: 10
    paths:
        - bootstrap
        - src
        - tests
        - config
        - public
        - support
    reportUnmatchedIgnoredErrors: true
    inferPrivatePropertyTypeFromConstructor: true
    treatPhpDocTypesAsCertain: false

    scanFiles:
        - tools/phpstan-stubs/optional-scaffolds.stub.php
        - tools/phpstan-stubs/optional-models.stub.php

    bootstrapFiles:
        - tools/phpstan-bootstrap.php
